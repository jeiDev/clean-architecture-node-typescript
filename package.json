{
  "name": "player-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npx tsc --watch\" \"nodemon",
    "start": "ts-node build/index.js",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d ./src/database/connections.ts",
    "migration:generate": "npm run typeorm migration:generate ./src/database/db/%npm_config_app%/migrations/%npm_config_schema%/%npm_config_name%.migration",
    "migration:create": "typeorm migration:create ./src/database/db/%npm_config_app%/migrations/%npm_config_schema%/%npm_config_name%.migration",
    "migration:show": "npm run typeorm migration:show",
    "migration:revert": "npm run typeorm migration:revert",
    "migration": "npm run typeorm migration:run",
    "model:create": "ts-node ./node_modules/typeorm/cli.js entity:create ./src/database/db/%npm_config_app%/entities/%npm_config_schema%/%typeDir%/%npm_config_name%.%npm_config_type%",
    "subscriber:create": "typeorm subscriber:create ./src/database/db/%npm_config_app%/subscribers/%npm_config_schema%/%npm_config_name%.subscriber",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/config/database/ormconfig/%npm_config_app%.ts"
  },
  "keywords": [],
  "author": "JeiDevp",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-session-sqlite": "^2.1.1",
    "helmet": "^6.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.7",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.18",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}
